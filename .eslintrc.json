{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
        "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-filename-extension": [ 1,
        {
            "extensions": [ ".js", ".jsx', '.test.js" ]
        }
    ],
    "arrow-parens": 0,
    "comma-dangle": 0,
    "import/no-named-as-default": 0,
    "no-undef": [ 1 ],
    "object-curly-newline": 0,
    "react/prefer-stateless-function": 0,
    "react/react-in-jsx-scope": 0
  }
}
